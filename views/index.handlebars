   <!--Jumbotron-->
    <header>
        <div class="jumbotron jumbotron-fluid">
            <div class="container-fluid">
                <h1 class="display-4">readit</h1>
                <p class="lead"> Definitely here before reddit.</p>
            </div>
        </div>
    </header>

    <section>
        <!-- Modal-->
        <!-- Button trigger modal -->
        
            <div class="row">
                <div class="col-xl-12">
                    <div class="modal fade" id="myThreadModal">
                        <div class="modal-dialog modal-lg">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h1>Thread Name</h1>
                                </div>
                                <div class="modal-body">
                                    Enter text...
                                    <input class="form-control">
                                </div>
                                
                                <div class="modal-footer">
                                    <input class="btn btn-primary" data-dismiss="modal" value="Close">
                                </div>
                            </div>
                        </div>
                    </div>
                    <a href="#" data-toggle="modal" data-target="#myThreadModal">New Thread</a>

                    <div class="modal fade" id="myPostModal">
                        <div class="modal-dialog modal-lg">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h1>New Post</h1>
                                </div>
                                <div class="modal-body">
                                    Username
                                    <input class="form-control">
                                </div>
                                <div class="modal-body">
                                    Title
                                    <input class="form-control">
                                </div>
                                <div class="modal-body">
                                    Enter text...
                                    <input class="form-control">
                                </div>
                                
                                <div class="modal-footer">
                                    <input class="btn btn-primary" data-dismiss="modal" value="Close">
                                </div>
                            </div>
                        </div>
                    </div>                    
                    <a href="#" data-toggle="modal" data-target="#myPostModal">New Post</a>


                    
                    <br><br>
                    <form class="create-form">
                        <div class="form-group">
                            <label for="ri">Search</label>
                            <input type="text" id="ri" name="thread">
                            <h4>Active Threads</h4>
                        </div>
                    </form>
                </div>
            </div>
        
    </section>


    <div class="">
        <div class="row" id="rowgap">
            <div class="col-xl-1">
                {{#each Thread}}
                {{> Thread/subThread}}
                {{/each}}
            </div>

